#: js/lang/calendar-lang.php
msgid "About the calendar"
msgstr "À propos du calendrier"

#: js/lang/calendar-lang.php
msgid "For latest version visit: %1"
msgstr "Pour la dernière version, visitez: %1"

#: js/lang/calendar-lang.php
msgid "Distributed under GNU LGPL. See %1 for details."
msgstr "Distribué par GNU LGPL. Voir %1 pour les details."

#: js/lang/calendar-lang.php
msgid "Date selection:"
msgstr "Sélection de la date:"

#: js/lang/calendar-lang.php
msgid "- use the \\xab, \\xbb buttons to select year,"
msgstr "- utilisez les boutons \\xab, \\xbb pour sélectionner l’année,"

#: js/lang/calendar-lang.php
msgid "- use the \\u2039, \\u203a buttons to select month,"
msgstr "- utilisez les boutons \\u2039, \\u203a pour sélectionner le mois,"

#: js/lang/calendar-lang.php
msgid "- hold mouse button on any of the above buttons for faster selection."
msgstr "- gardez la souris sur n’importe quel de ces boutons pour une sélection plus rapide."

#: js/lang/calendar-lang.php
msgid "Time selection:"
msgstr "Sélection de l’heure:"

#: js/lang/calendar-lang.php
msgid "- click on any of the time parts to increase it"
msgstr "- Cliquez sur les heures ou les minutes pour incrémenter"

#: js/lang/calendar-lang.php
msgid "- or Shift-click to decrease it"
msgstr "- ou cliquez en maintenant la touche Majuscule enfoncée (Maj-clic) pour décrémenter"

#: js/lang/calendar-lang.php
msgid "- or click and drag for faster selection."
msgstr "- ou cliquez-déplacez pour une sélection plus rapide."

#: js/lang/calendar-lang.php
msgid "hold for menu"
msgstr "maintenir pour menu"

#: js/lang/calendar-lang.php
msgid "Prev. year"
msgstr "Année préc."

#: js/lang/calendar-lang.php
msgid "Prev. month"
msgstr "Mois préc."

#: js/lang/calendar-lang.php
msgid "Go Today"
msgstr "Atteindre la date du jour"

#: js/lang/calendar-lang.php
msgid "Next month"
msgstr "Mois suiv."

#: js/lang/calendar-lang.php
msgid "Next year"
msgstr "Année suiv."

#: js/lang/calendar-lang.php
msgid "Select date"
msgstr "Sélectionner une date"

#: js/lang/calendar-lang.php
msgid "Drag to move"
msgstr "Déplacer"

#: js/lang/calendar-lang.php
msgid "today"
msgstr "Aujourd’hui"

#: js/lang/calendar-lang.php
msgid "Display %s first"
msgstr "Afficher %s en premier"

#: js/lang/calendar-lang.php
msgid "Close"
msgstr "Fermer"

#: js/lang/calendar-lang.php
msgid "Today"
msgstr "Aujourd’hui"

#: js/lang/calendar-lang.php
msgid "(Shift-)Click or drag to change value"
msgstr "(Maj-)Clic ou glisser pour modifier la valeur"

#: js/lang/calendar-lang.php
msgid "wk"
msgstr "Sem."

#: js/lang/calendar-lang.php
msgid "Time:"
msgstr "Heure:"

