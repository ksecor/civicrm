#: js/lang/calendar-lang.php
msgid "About the calendar"
msgstr "Om Kalenderen"

#: js/lang/calendar-lang.php
msgid "For latest version visit: %1"
msgstr "For den seneste version besøg: %1"

#: js/lang/calendar-lang.php
msgid "Distributed under GNU LGPL. See %1 for details."
msgstr "Distribueret under GNU LGPL. Se %1 for detajler."

#: js/lang/calendar-lang.php
msgid "Date selection:"
msgstr "Valg af dato:"

#: js/lang/calendar-lang.php
msgid "- use the \\xab, \\xbb buttons to select year,"
msgstr "- Brug \\xab, \\xbb knapperne for at vælge år"

#: js/lang/calendar-lang.php
msgid "- use the \\u2039, \\u203a buttons to select month,"
msgstr "- Brug \\u2039, \\u203a apperne for at vælge måned"

#: js/lang/calendar-lang.php
msgid "- hold mouse button on any of the above buttons for faster selection."
msgstr "- Hold knappen på musen nede på knapperne ovenfor for hurtigere valg."

#: js/lang/calendar-lang.php
msgid "Time selection:"
msgstr "Valg af tid:"

#: js/lang/calendar-lang.php
msgid "- click on any of the time parts to increase it"
msgstr "- Klik på en vilkårlig del for større værdi"

#: js/lang/calendar-lang.php
msgid "- or Shift-click to decrease it"
msgstr "- eller Shift-klik for for mindre værdi"

#: js/lang/calendar-lang.php
msgid "- or click and drag for faster selection."
msgstr "- eller klik og træk for hurtigere valg."

#: js/lang/calendar-lang.php
msgid "hold for menu"
msgstr "hold for menu"

#: js/lang/calendar-lang.php
msgid "Prev. year"
msgstr "Ét år tilbage"

#: js/lang/calendar-lang.php
msgid "Prev. month"
msgstr "Én måned tilbage"

#: js/lang/calendar-lang.php
msgid "Go Today"
msgstr "Gå til i dag"

#: js/lang/calendar-lang.php
msgid "Next month"
msgstr "Én måned frem"

#: js/lang/calendar-lang.php
msgid "Next year"
msgstr "Ét år frem"

#: js/lang/calendar-lang.php
msgid "Select date"
msgstr "Vælg dag"

#: js/lang/calendar-lang.php
msgid "Drag to move"
msgstr "Træk vinduet"

#: js/lang/calendar-lang.php
msgid "today"
msgstr "i dag"

#: js/lang/calendar-lang.php
msgid "Display %s first"
msgstr "Vis %s først"

#: js/lang/calendar-lang.php
msgid "Close"
msgstr "Luk"

#: js/lang/calendar-lang.php
msgid "Today"
msgstr "I dag"

#: js/lang/calendar-lang.php
msgid "(Shift-)Click or drag to change value"
msgstr "(Shift-)klik eller træk for at ændre værdi"

#: js/lang/calendar-lang.php
msgid "wk"
msgstr "Uge"

#: js/lang/calendar-lang.php
msgid "Time:"
msgstr "Tid:"
